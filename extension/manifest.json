{
  "description": "Interface between competitive programming websites and your code editor",
  "manifest_version": 3,
  "name": "CPTerm",
  "version": "1.0",
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "*://*.hackerrank.com/*",
        "*://*.leetcode.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "scraper-inject.js"
      ],
      "matches": [
        "*://*.hackerrank.com/*",
        "*://*.leetcode.com/*"
      ]
    }
  ],
  "permissions": [
    "nativeMessaging",
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "*://*.hackerrank.com/*",
    "*://*.leetcode.com/*"
  ]
}